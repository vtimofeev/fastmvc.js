module ui {
  export class {className} extends {?extend}{extend}{:else}fmvc.View{/extend}
  {
    {#links}
    public {name}:Element;
    {/links}

    constructor(name:string, modelOrData?:fmvc.Model|any, jsTemplate?:fmvc.IDomObject) {
           super(name, modelOrData, jsTemplate);
    }

    createDom() {
      this.element = this.templateElement;
      {#links}
      this.{name} = this.elementPaths[{value|js|s}];
      {/links}
      this.childrenContainer = this.childrenContainer || this.element;
      return this;
    }

    public get jsTemplate():fmvc.IRootDomObject {
          return {className}.__jsTemplate;
    }

    private static __jsTemplate:fmvc.IRootDomObject = {.|js|s};
  }

}

