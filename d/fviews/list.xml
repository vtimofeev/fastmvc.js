
<div
        class="list"
        css="./list.css"

        base="list"
        className="fmvc.examples.List"
        extend="fmvc.ListView"

        enableStates="hover,active,error,disabled,selected,custom1,custom2"
        enableActions="pan,pinch,rotate"

        childrenClass="fmvc.examples.ListItem"
        >
    <h1>{title} {modelList.length}</h1>
    <div class="*-container"
         fieldName="children">
    </div>

    <div extend="fmvc.Group" action="action" groupType="one">
        <div extend="fmvc.Button" action="action">{l18n:list.filterByDate}</div>
        <div extend="fmvc.Button" action="action">{l18n:list.filterByDate}</div>
    </div>

</div>