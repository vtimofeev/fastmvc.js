.userview {
  background-color: #808080;
  font-size: 16px;
  line-height: 16*1.5px;
  display: inline-block;
  width: 200px;
}

.userview-selected {
  border: 1px solid red;
}

.userview-hover {
  font-weight: bold;
  background-color: #0e90d2 !important;
  border: 1px solid blue;
}

.userview-disabled {
  font-color:#FF0000;
  text-decoration: underline;
}